<odoo>
  <record id="view_appointment_prebook_list" model="ir.ui.view">
    <field name="name">appointment.prebook.list</field>
      <field name="model">appointment.prebook</field>
      <field name="type">list</field>
      <field name="arch" type="xml">
        <list string="Prebook Logs">
        <field name="slot_id"/>
        <field name="visitor_id"/>
        <field name="action"/>
        <field name="create_date"/>
      </list>
    </field>
  </record>

  <record id="action_appointment_prebook_list" model="ir.actions.act_window">
    <field name="name">Appointment Prebook Logs</field>
    <field name="res_model">appointment.prebook</field>
    <field name="view_mode">list,form</field>
    <field name="help" type="html">
      <p>Track all visitor actions on appointment forms.</p>
    </field>
  </record>

  <menuitem id="menu_appointment_prebook_root" name="Prebook Logs" parent="website_appointment.menu_website_appointment" sequence="50"/>
  <menuitem id="menu_appointment_prebook_logs" name="Logs" parent="menu_appointment_prebook_root" action="action_appointment_prebook_list"/>
</odoo>